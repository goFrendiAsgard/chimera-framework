ins: action, migrationConfig
out: output
do:
    - if: action == 'up'
      do:
        - ins:
            - name: 'Landing page'
              route: '/'
              method: 'all'
              chain: '<%= chainPath %>index.chiml'
              defaultRoute: 1
            - name: 'Login API'
              route: '/login-api'
              method: 'all'
              chain: '<%= chainPath %>core.login-api.js'
              defaultRoute: 1
            - name: 'Logout API'
              route: '/logout-api'
              method: 'all'
              chain: '<%= chainPath %>core.logout-api.js'
              defaultRoute: 1
            - name: 'Register API'
              route: '/register-api'
              method: 'all'
              chain: '<%= chainPath %>core.register-api.js'
              defaultRoute: 1
            - name: 'Login page'
              route: '/login'
              method: 'get'
              chain: '<%= chainPath %>core.login-page.js'
              view: 'default.login-page.ejs'
              defaultRoute: 1
            - name: 'Login action'
              route: '/login'
              method: 'post'
              chain: '<%= chainPath %>core.login.js'
              view: 'default.login.ejs'
              defaultRoute: 1
            - name: 'Register page'
              route: '/register'
              method: 'get'
              chain: '<%= chainPath %>core.register-page.js'
              view: 'default.register-page.ejs'
              defaultRoute: 1
            - name: 'Register action'
              route: '/register'
              method: 'post'
              chain: '<%= chainPath %>core.register.js'
              view: 'default.register.ejs'
              defaultRoute: 1
            - name: 'Logout action'
              route: '/logout'
              method: 'all'
              chain: '<%= chainPath %>core.logout.js'
              view: 'default.logout.ejs'
              defaultRoute: 1
          out: defaultRoutes
        - |('web_routes', 'insert', defaultRoutes) -> [$.helper.mongoExecute] -> output

      else:
        - |('web_routes', 'remove', {'defaultRoute': 1}) -> [$.helper.mongoExecute] -> output
